{
  "compilerOptions": {
		"target": "ES2017",
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"baseUrl": "src",
		"paths": {
			"@/*": ["src/*"],
			"shadcn/*": ["src/components/shadcn/*"],
			"components/*": ["src/components/*"],
			"providers/*": ["src/providers/*"],
			"libs/*": ["./libs/*"],
			"icons": ["assets/icons"]
		}
	},
  }
  