{
  "compilerOptions": {
		"target": "ES2017",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": false,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
    "baseUrl": "src",
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
      "@/*": ["src/*"],
      "providers/*": ["./providers/*"],
			"libs/utils": ["./libs/utils"], 
			"icons": ["assets/icons"],
		},
		
	},
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/layout.jsx", "src/app/cuk/page.jsx", "libs/analytics/analytics-provider.js"],

  "exclude": ["node_modules"]
}
